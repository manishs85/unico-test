<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.unico.common.Gcd" table="gcd">
		<id name="id" type="java.lang.Integer">
			<column name="ID" />
			<generator class="identity" />
		</id>
		<property name="gcd" type="java.lang.Integer">
			<column name="gcd" not-null="true" />
		</property>
	</class>
</hibernate-mapping>